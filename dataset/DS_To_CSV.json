			"referenceName": "LS_ADLS_eylearningstorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ds_Folder_Name": {
				"type": "string"
			},
			"ds_FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().ds_FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().ds_Folder_Name",
					"type": "Expression"
				},
				"fileSystem": "sql-tables-data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}